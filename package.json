{
  "name": "duo-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun x prisma generate",
    "start": "bun src/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "bun x prisma generate",
    "db:push": "bun x prisma db push",
    "db:migrate": "bun x prisma migrate dev",
    "db:seed": "bun prisma/seed.ts",
    "test": "bun x vitest",
    "test:run": "bun x vitest run",
    "test:coverage": "bun x vitest run --coverage"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/sensible": "^6.0.4",
    "@prisma/client": "^6.1.0",
    "fastify": "^5.2.1",
    "fastify-type-provider-zod": "^6.0.0",
    "firebase-admin": "^13.6.0",
    "pino": "^10.0.0",
    "pino-pretty": "^12.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "17.2.3",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^4.0.15"
  }
}
